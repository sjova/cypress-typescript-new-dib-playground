{
  "name": "cypress-typescript-dib-playground",
  "version": "1.0.1",
  "description": "Cypress TypeScript DIB Playground",
  "private": true,
  "engines": {
    "node": ">=16.5.0",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:dev": "cypress open --config testFiles=dev-playground/**/*.spec.ts",
    "cy:open:dib": "cypress open --config testFiles=dib-travel/**/*.spec.ts",
    "cy:open:dib:iframe": "cypress open --config testFiles=dib-travel-iframe/**/*.spec.ts,chromeWebSecurity=false,blockHosts=null,defaultCommandTimeout=40000,requestTimeout=50000",
    "cy:open:dib:dev": "CYPRESS_BASE_URL=https://develop--dib-travel.netlify.app/ cypress open --config testFiles=dib-travel/**/*.spec.ts",
    "cy:open:dib:iframe:dev": "CYPRESS_BASE_URL=https://develop--dib-travel.netlify.app/ cypress open --config testFiles=dib-travel-iframe/**/*.spec.ts,chromeWebSecurity=false,blockHosts=null,defaultCommandTimeout=40000,requestTimeout=50000",
    "cy:run:dev": "cypress run --spec 'cypress/integration/dev-playground/*.spec.ts'",
    "cy:run:dib": "cypress run --spec 'cypress/integration/dib-travel/**/*.spec.ts'",
    "cy:run:dib:iframe": "cypress run --config chromeWebSecurity=false,blockHosts=null,defaultCommandTimeout=40000,requestTimeout=50000 --spec 'cypress/integration/dib-travel-iframe/**/*.spec.ts'",
    "cy:run:dib:dev": "CYPRESS_BASE_URL=https://develop--dib-travel.netlify.app/ cypress run --spec 'cypress/integration/dib-travel/**/*.spec.ts'",
    "cy:run:dib:iframe:dev": "CYPRESS_BASE_URL=https://develop--dib-travel.netlify.app/ cypress run --config chromeWebSecurity=false,blockHosts=null,defaultCommandTimeout=40000,requestTimeout=50000 --spec 'cypress/integration/dib-travel-iframe/**/*.spec.ts'",
    "cy:run:dib:cars": "cypress run --spec 'cypress/integration/dib-travel/cars/**/*.spec.ts'",
    "cy:run:dib:company-employees": "cypress run --spec 'cypress/integration/dib-travel/company-employees/**/*.spec.ts'",
    "cy:run:dib:company-report": "cypress run --spec 'cypress/integration/dib-travel/company-report/**/*.spec.ts'",
    "cy:run:dib:company-settings": "cypress run --spec 'cypress/integration/dib-travel/company-settings/**/*.spec.ts'",
    "cy:run:dib:cy-accounts-fixture": "cypress run --spec 'cypress/integration/dib-travel/cy-accounts-fixture/**/*.spec.ts'",
    "cy:run:dib:flights": "cypress run --spec 'cypress/integration/dib-travel/flights/**/*.spec.ts'",
    "cy:run:dib:forgot-password": "cypress run --spec 'cypress/integration/dib-travel/**/*.spec.ts'",
    "cy:run:dib:group": "cypress run --spec 'cypress/integration/dib-travel/group/**/*.spec.ts'",
    "cy:run:dib:hotels": "cypress run --spec 'cypress/integration/dib-travel/hotels/**/*.spec.ts'",
    "cy:run:dib:my-travels": "cypress run --spec 'cypress/integration/dib-travel/my-travels/**/*.spec.ts'",
    "cy:run:dib:other": "cypress run --spec 'cypress/integration/dib-travel/other/**/*.spec.ts'",
    "cy:run:dib:personal-settings": "cypress run --spec 'cypress/integration/dib-travel/personal-settings/**/*.spec.ts'",
    "cy:run:dib:rail-bus": "cypress run --spec 'cypress/integration/dib-travel/rail-bus/**/*.spec.ts'",
    "cy:run:dib:sign-in": "cypress run --spec 'cypress/integration/dib-travel/sign-in/**/*.spec.ts'",
    "cy:run:dib:sign-up": "cypress run --spec 'cypress/integration/dib-travel/sign-up/**/*.spec.ts'",
    "cy:run:all": "cypress run",
    "cy:spec": "cypress run --spec",
    "cy:report:delete": "rm -Rf cypress/results || rm -Rf mochawesome-report || true",
    "cy:report:merge:junit": "jrm ./cypress/results/combined-junit.xml \"./cypress/results/*.xml\"",
    "cy:report:merge:mochawesome": "mochawesome-merge ./cypress/results/*.json > ./cypress/results/combined-mochawesome.json",
    "cy:report:mochawesome": "marge cypress/results/combined-mochawesome.json",
    "cy:report": "npm run cy:report:merge:junit && npm run cy:report:merge:mochawesome && npm run cy:report:mochawesome",
    "eslint": "eslint cypress --ext ts --fix",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjova/cypress-typescript.git"
  },
  "keywords": [
    "Cypress",
    "TypeScript"
  ],
  "author": "Saša Jovanović",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjova/cypress-typescript/issues"
  },
  "homepage": "https://github.com/sjova/cypress-typescript",
  "dependencies": {
    "cypress": "9.2.0"
  },
  "devDependencies": {
    "@types/node": "16.11.17",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "cypress-multi-reporters": "1.5.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-mocha": "10.0.3",
    "husky": "7.0.4",
    "junit-report-merger": "3.0.4",
    "lint-staged": "12.1.4",
    "mocha": "9.1.3",
    "mocha-junit-reporter": "2.0.2",
    "mochawesome": "7.0.1",
    "mochawesome-merge": "4.2.1",
    "mochawesome-report-generator": "6.0.1",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  }
}
