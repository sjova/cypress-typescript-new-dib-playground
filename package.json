{
  "name": "cypress-typescript-dib-playground",
  "version": "1.0.1",
  "description": "Cypress TypeScript DIB Playground",
  "private": true,
  "engines": {
    "node": ">=14.18.1",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "cy:open:dev": "cypress open --config testFiles=dev-playground/**/*.spec.ts",
    "cy:open:dib:iframe": "cypress open --config testFiles=dib-travel-iframe/**/*.spec.ts,chromeWebSecurity=false",
    "cy:open:dib": "cypress open --config testFiles=dib-travel/**/*.spec.ts",
    "cy:open": "cypress open",
    "cy:run:all": "cypress run",
    "cy:run:dev": "cypress run --spec 'cypress/integration/dev-playground/*.spec.ts'",
    "cy:run:dib:iframe": "cypress run --config chromeWebSecurity=false --spec 'cypress/integration/dib-travel-iframe/**/*.spec.ts'",
    "cy:run:dib": "cypress run --spec 'cypress/integration/dib-travel/**/*.spec.ts'",
    "cy:spec": "cypress run --spec",
    "eslint": "eslint cypress --ext ts --fix",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "test:ci": "CYPRESS_BASE_URL=https://ci.app.com cypress open",
    "test:staging": "CYPRESS_BASE_URL=https://staging.app.com cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjova/cypress-typescript.git"
  },
  "keywords": [
    "Cypress",
    "TypeScript"
  ],
  "author": "Saša Jovanović",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjova/cypress-typescript/issues"
  },
  "homepage": "https://github.com/sjova/cypress-typescript",
  "dependencies": {
    "cypress": "8.7.0"
  },
  "devDependencies": {
    "@types/node": "14.17.32",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-mocha": "9.0.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  }
}
